{
  "name": "tobikaleigh/laravel-actions",
  "description": "Use app/actions as a way to organize your Laravel application",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Tobikaleigh\\Actions\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tobikaleigh\\Actions\\Test\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Tobias Lund Nielsen",
      "email": "tobikaleigh@gmail.com"
    }
  ],
  "require": {
    "illuminate/collections": "^10.0 || ^11.0",
    "illuminate/queue": "^10.0 || ^11.0",
    "illuminate/database": "^10.0 || ^11.0",
    "illuminate/config": "^10.0 || ^11.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "require-dev": {
    "pestphp/pest": "^2.34"
  }
}
